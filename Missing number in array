// User function template for C++

class Solution{
  public:
    int MissingNumber(vector<int>& array, int n) {
        // Your code goes here
        int sum1 = n*(n+1)/2;
        int sum2 = 0;
        for(int i=0 ;i<n-1 ;i++)
        {
            sum2 = sum2+array[i];
        }
        return sum1 - sum2 ;
    
    }
};
